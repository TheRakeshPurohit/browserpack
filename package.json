{
  "name": "browserpack",
  "version": "0.0.1",
  "description": "hackable browser based bundler inspired from webpack",
  "main": "index.js",
  "scripts": {
    "dev": "parcel ./index.html",
    "prettify": "prettier --write",
    "lint": "eslint . --fix",
    "lint:staged": "eslint --fix"
  },
  "repository": "https://github.com/ameerthehacker/browser-bundler.git",
  "author": "ameerthehacker",
  "license": "apache",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "eslint": "^6.8.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1"
  },
  "lint-staged": {
    "*.{js,ts,html,json,scss,css,md,yml}": [
      "yarn prettify",
      "git add"
    ],
    "*.{spec,js}": [
      "yarn lint:staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.7.7",
    "acorn": "^7.1.0",
    "babel-preset-es": "^1.0.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
